    </table>

    <a href="{{ path('app_promo_vols_index') }}">back to list</a>

    <a href="{{ path('app_promo_vols_edit', {'id': promo_vol.id}) }}">edit</a>

    {{ include('promo_vols/_delete_form.html.twig') }}
