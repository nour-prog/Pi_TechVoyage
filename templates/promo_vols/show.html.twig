<title>PromoVols</title>
{% endblock %}
{% block body %}
    <h1>PromoVols</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ promo_vol.id }}</td>
            </tr>
            <tr>
                <th>Pourcentage</th>
                <td>{{ promo_vol.pourcentage }}</td>
